{
  "$schema": "http://json.schemastore.org/sarif-2.1.0",
  "version": "2.1.0",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "Ghidra",
          "version": "11.0.1",
          "informationUri": "https://github.com/NationalSecurityAgency/ghidra",
          "rules": [
            {
              "id": "COMMENTS",
              "name": "comment",
              "defaultConfiguration": {
                "level": "NONE",
                "kind": "INFORMATION"
              },
              "fullDescription": {
                "text": "[COMMENT]"
              }
            },
            {
              "id": "BOOKMARKS",
              "name": "bookmark",
              "defaultConfiguration": {
                "level": "NONE",
                "kind": "INFORMATION"
              },
              "fullDescription": {
                "text": "[BOOKMARK]"
              }
            },
            {
              "id": "CODE",
              "name": "code",
              "defaultConfiguration": {
                "level": "NONE",
                "kind": "INFORMATIONAL"
              },
              "fullDescription": {
                "text": "[CODE]"
              }
            }
          ]
        }
      }
    }
  ]
}
