{
  "$schema": "http://json.schemastore.org/sarif-2.1.0",
  "version": "2.1.0",
  "runs": [
    {
      "properties": {
	 "name": "radare2",
	 "version": "5.9.1"
      },
      "tool": {
        "driver": {
          "name": "NowSecure SARIF Findings",
          "semanticVersion": "0.0.1",
          "rules": [
            {
              "id": "EXAMPLE-VULN-001",
              "shortDescription": {
                "text": "Potential buffer overflow."
              },
              "helpUri": "http://example.com/vulnerability/EXAMPLE-VULN-001"
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "EXAMPLE-VULN-001",
          "level": "error",
          "message": {
            "text": "Buffer overflow vulnerability detected."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "binary://example-binary",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "byteOffset": 1024,
                  "byteLength": 128
                }
              },
              "properties": {
                "memoryAddress": "0x0040321A"
              }
            }
          ]
        }
      ]
    }
  ]
}
